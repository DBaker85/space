{
   "locales": ["en", "fr"],
   "catalogs": [{
      "path": "src/locales/{locale}/messages",
      "include": ["src"]
   }],
   "format": "po",
   "extractBabelOptions": {
    "plugins": ["@babel/plugin-syntax-dynamic-import"],
    "presets": ["@babel/preset-typescript", "@babel/preset-react", "@lingui/babel-preset-react"]
  }
}